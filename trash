local function GHUHD_fake_script() -- spam.code made 100% by me 
	local script = Instance.new('LocalScript', spam)

	script.Parent.MouseButton1Down:Connect(function()
	local mouse = game.Players.LocalPlayer:GetMouse()
	local spamtool = Instance.new("Tool")
	spamtool.RequiresHandle = false
	spamtool.Name = 'Spam Tool'
	spamtool.ToolTip = "Uses every attack in the server."
	spamtool.CanBeDropped = false
	spamtool.Activated:connect(function()
		local pos = mouse.Hit+Vector3.new(0,2.5,0)
	
		for Index, Value in next, game:GetDescendants() do
			if Value:IsA("Tool") then
				if Value:FindFirstChild("RemoteEvent") then
					if Value.Name ~= "Double Cherry" and Value.Name ~= "DETERMINATION" and Value.Name ~= "Sickness" and Value.Name ~= "Dodge" and Value.Name ~= "Block" then
						Value.RemoteEvent:FireServer(pos.X,pos.Y,pos.Z);
					end
				end;
			end;
		end;
	end)
	spamtool.Parent = game.Players.LocalPlayer.Backpack
	end)
	
	
end
coroutine.wrap(GHUHD_fake_script)()
