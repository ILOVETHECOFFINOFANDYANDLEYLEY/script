local function LMWQ_fake_script() -- teleportsswitch.switch to teleports 
	local script = Instance.new('LocalScript', teleportsswitch)

	script.Parent.MouseButton1Down:Connect(function()
		if script.Parent.Parent.Parent.thehackedition.Visible == true then
			script.Parent.Parent.Parent.thehackedition.Visible = false
			if script.Parent.Parent.Parent.thehackedition.Active == true then
				script.Parent.Parent.Parent.thehackedition.Active = false
				
				
				
				
			end
		end
	end)
	
	
end
coroutine.wrap(LMWQ_fake_script)()
