-- Scripts:

local function LWMY_fake_script() -- Snowdin.LocalScript 
	local script = Instance.new('LocalScript', Snowdin)

	script.Parent.MouseButton1Click:connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(894, 35, 31)
	end)
	
end
coroutine.wrap(LWMY_fake_script)()
